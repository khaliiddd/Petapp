# Production Environment Configuration
NODE_ENV=production
PORT=3000

# Security - Generate new secrets for production
JWT_SECRET=petrental-jwt-secret-32-char-min-key-production-2025
SESSION_SECRET=petrental-session-secret-32-char-min-key-2025

# Database - SQLite for MVP (upgrade to PostgreSQL for scale)
DB_PATH=./pet_rental.db

# CORS Settings
CORS_ORIGIN=https://your-app.railway.app

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100

# Logging
LOG_LEVEL=info

# Demo Data
LOAD_DEMO_DATA=true
DEMO_USERS=3
DEMO_EQUIPMENT=9